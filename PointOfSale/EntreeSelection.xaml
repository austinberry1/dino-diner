<Page x:Class="PointOfSale.EntreeSelection"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PointOfSale"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EntreeSelection">


    <WrapPanel>
        <WrapPanel.Resources>
            <Style TargetType="Button">
                <Setter Property="MinWidth" Value="150"/>
                <Setter Property="MinHeight" Value="150"/>
                <Setter Property="Margin" Value="20"/>
               
            </Style>
        </WrapPanel.Resources>
        <Button  Content="BrontoWurst" />
        <Button  Content="Dino-Nuggets" />
        <Button Content="Prehistoric PB&amp;J" />
        <Button  Content="Pterodactyl Wings" />
        <Button  Content="Steakosaurus Buger" />
        <Button  Content="T-Rex KingBurger" />
        <Button  Content="Veloci-Wrap" />
    </WrapPanel>
</Page>
